        <term_definitions code="at0000">
          <items id="description">Summary or persisting information about tobacco use or consumption.</items>
          <items id="text">Tobacco Use Summary</items>
        </term_definitions>
        <term_definitions code="at0001">
          <items id="description">@ internal @</items>
          <items id="text">Tree</items>
        </term_definitions>
        <term_definitions code="at0002">
          <items id="description">Statement about the individual's current tobacco smoking activity.</items>
          <items id="text">Smoking Status</items>
        </term_definitions>
        <term_definitions code="at0003">
          <items id="description">Individual is a current smoker of tobacco.</items>
          <items id="text">Current Smoker</items>
        </term_definitions>
        <term_definitions code="at0005">
          <items id="description">Individual is a former or ex-smoker of tobacco.</items>
          <items id="text">Ex-smoker</items>
        </term_definitions>
        <term_definitions code="at0006">
          <items id="description">Individual has never smoked tobacco.</items>
          <items id="text">Never Smoked</items>
        </term_definitions>
        <term_definitions code="at0015">
          <items id="comment">Can be a partial date, for example, only a year.</items>
          <items id="description">Date that smoking commenced.</items>
          <items id="text">Date Commenced</items>
        </term_definitions>
        <term_definitions code="at0016">
          <items id="comment">Can be a partial date, for example, only a year. May also be known as the 'Quit Date'.</items>
          <items id="description">Date that smoking ceased.</items>
          <items id="text">Date Ceased</items>
        </term_definitions>
        <term_definitions code="at0017">
          <items id="comment">Commonly used in assessment of cigarette use. It is calculated by multiplying the number of packs of cigarettes smoked per day by the number of years the individual has smoked. One pack year equals 365 packs of cigarettes.</items>
          <items id="description">Estimate of the cumulative amount of cigarettes smoked, measured in pack years.</items>
          <items id="text">Pack Years</items>
        </term_definitions>
        <term_definitions code="at0019">
          <items id="comment">For example: stopped smoking or reduced amount on becoming pregnant.</items>
          <items id="description">Additional narrative about the toacco use not captured in other fields.</items>
          <items id="text">Comment</items>
        </term_definitions>
        <term_definitions code="at0021">
          <items id="description">@ internal @</items>
          <items id="text">Tree</items>
        </term_definitions>
        <term_definitions code="at0022">
          <items id="description">The date this tobacco use summary was last updated.</items>
          <items id="text">Date Last Updated</items>
        </term_definitions>
        <term_definitions code="at0023">
          <items id="comment">This data element allows a rough indication of cigarette use to be recorded in a number of formats. For example: 30 a day (quantity) or 5-10 per day (interval of quantity), or select from terminology value set (either a set specified in the template or from a specified external terminology reference set). Please note: the period of time to which this data element is not specified. If exact consumption at specific points in time or averages/maximums over specified intervals of time are required, use the OBSERVATION.tobacco_use archetype.</items>
          <items id="description">Estimate of typical use of the form of smoked tobacco per day - as a whole number, a range or as a term (usually coded).</items>
          <items id="text">Typical Smoked Amount</items>
        </term_definitions>
        <term_definitions code="at0024">
          <items id="comment">For example chewing tobacco.</items>
          <items id="description">Narrative description of other tobacco use, other than smoking.</items>
          <items id="text">Other Tobacco Use</items>
        </term_definitions>
        <term_definitions code="at0025">
          <items id="description">Number of times the individual has attempted to stop smoking.</items>
          <items id="text">Number of Cessation Attempts</items>
        </term_definitions>
        <term_definitions code="at0026">
          <items id="description">Details about each attempt to cease  smoking.</items>
          <items id="text">Cessation Attempts</items>
        </term_definitions>
        <term_definitions code="at0027">
          <items id="description">Individual has ceased smoking tobacco within the previous 12 months.</items>
          <items id="text">Quitting</items>
        </term_definitions>
        <term_definitions code="at0028">
          <items id="comment">The geographical and cultural variation of commonly used forms varies enormously and so this value set should not be specified in this archetype, however coding of the Form with a terminology is desirable, where possible.</items>
          <items id="description">Identification of the form of tobacco smoked.</items>
          <items id="text">Form</items>
        </term_definitions>
        <term_definitions code="at0029">
          <items id="description">Details about the pattern of use of a specified form of smoked tobacco.</items>
          <items id="text">Smoking Details</items>
        </term_definitions>
        <term_definitions code="at0030">
          <items id="comment">Definitions of 'occasional use' vary widely in research and literature.</items>
          <items id="description">The typical pattern of use of the form of smoked tobacco.</items>
          <items id="text">Pattern of Use</items>
        </term_definitions>
        <term_definitions code="at0031">
          <items id="description">The subject uses the tobacco form on a regular basis, generally daily.</items>
          <items id="text">Regular, daily use</items>
        </term_definitions>
        <term_definitions code="at0032">
          <items id="description">The subject of care uses the tobacco form occasionally.</items>
          <items id="text">Occasional use</items>
        </term_definitions>
        <term_definitions code="at0033">
          <items id="description">Statement about the individual's current snuff use.</items>
          <items id="text">Snuff use status</items>
        </term_definitions>
        <term_definitions code="at0034">
          <items id="description">Details of the subject's use of snuff.</items>
          <items id="text">Snuff use details</items>
        </term_definitions>
        <term_definitions code="at0035">
          <items id="description">The typical use of snuff measured in tins used per week.</items>
          <items id="text">Typical Snuff Consumption (tins)</items>
        </term_definitions>
        <term_definitions code="at0036">
          <items id="description">The subject uses 1-3 tins of snuff per week.</items>
          <items id="text">1-3 tins per week</items>
        </term_definitions>
        <term_definitions code="at0037">
          <items id="description">The subject uses 4-6 tins per week.</items>
          <items id="text">4-6 tins per week</items>
        </term_definitions>
        <term_definitions code="at0038">
          <items id="description">The subject uses more than 7 tins of snuff per week.</items>
          <items id="text">+7 tins per week</items>
        </term_definitions>
        <term_definitions code="at0039">
          <items id="description">The subject's pattern of use of snuff.</items>
          <items id="text">Pattern of Use</items>
        </term_definitions>
        <term_definitions code="at0040">
          <items id="description">The subject is currently using snuff.</items>
          <items id="text">Current snuff user</items>
        </term_definitions>
        <term_definitions code="at0041">
          <items id="description">The subject formerly used snuff.</items>
          <items id="text">Former snuff user</items>
        </term_definitions>
        <term_definitions code="at0042">
          <items id="description">The subject has never used snuff.</items>
          <items id="text">Never used snuff</items>
        </term_definitions>
      </children>
