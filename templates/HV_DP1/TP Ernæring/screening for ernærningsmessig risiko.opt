        <term_definitions code="at0000">
          <items id="description">Verktøyet Nutritional Risk Screening (NRS) 2002 for screening etter feilernæring.</items>
          <items id="text">Nutritional Risk Screening</items>
        </term_definitions>
        <term_definitions code="at0001">
          <items id="description">@ internal @</items>
          <items id="text">Event Series</items>
        </term_definitions>
        <term_definitions code="at0002">
          <items id="description">Standard, uspesifisert tidspunkt eller tidsintervall som kan defineres mer eksplisitt i en template eller i en applikasjon.</items>
          <items id="text">Uspesifisert hendelse</items>
        </term_definitions>
        <term_definitions code="at0003">
          <items id="description">@ internal @(nb).</items>
          <items id="text">Tree</items>
        </term_definitions>
        <term_definitions code="at0004">
          <items id="description">Er pasientens BMI under 20,5?</items>
          <items id="text">Er BMI &lt; 20,5?</items>
        </term_definitions>
        <term_definitions code="at0005">
          <items id="description">Vekttap de siste ukene?</items>
          <items id="text">Har pasienten tapt vekt de siste tre månedene?</items>
        </term_definitions>
        <term_definitions code="at0006">
          <items id="description">Redusert ernæringsinntak siste uken?</items>
          <items id="text">Har pasienten hatt redusert næringsinntak den siste uken?</items>
        </term_definitions>
        <term_definitions code="at0007">
          <items id="description">Alvorlig sykdom? (Eksempel intensivopphold).</items>
          <items id="text">Er pasienten alvorlig/kritisk syk?</items>
        </term_definitions>
        <term_definitions code="at0016">
          <items id="description">Kartlegging av ernæringstilstand.</items>
          <items id="text">Ernæringstilstand</items>
        </term_definitions>
        <term_definitions code="at0017">
          <items id="description">Kartlegging av syklighetsgrad.</items>
          <items id="text">Sykdommens alvorlighetsgrad</items>
        </term_definitions>
        <term_definitions code="at0018">
          <items id="description">Normal</items>
          <items id="text">Score 0 - Normal ernæringsstatus</items>
        </term_definitions>
        <term_definitions code="at0019">
          <items id="description">Mild</items>
          <items id="text">Score 1 - Vekttap &gt; 5 % siste 3 mnd. eller matinntak &lt; 50 – 75 % av behov siste uke.</items>
        </term_definitions>
        <term_definitions code="at0020">
          <items id="description">Moderat</items>
          <items id="text">Score 2 - Vekttap &gt; 5 % siste 2 mnd. eller BMI 18.5 – 20.5 og redusert allmenntilstand eller matinntak 25–50 % av behov sist uke.</items>
        </term_definitions>
        <term_definitions code="at0021">
          <items id="description">Alvorlig</items>
          <items id="text">Score 3 - Vekttap &gt; 5 % siste mnd. eller BMI &lt; 18.5 og redusert allmenntilstand eller matinntak 0–25 % av behov siste uke.</items>
        </term_definitions>
        <term_definitions code="at0022">
          <items id="description">Normal</items>
          <items id="text">Score 0 - Normal ernæringsbehov</items>
        </term_definitions>
        <term_definitions code="at0023">
          <items id="description">Mild</items>
          <items id="text">Score 1 - En pasient med kronisk sykdom eller en pasient som har 
gjennomgått et mindre kirurgisk inngrep.</items>
        </term_definitions>
        <term_definitions code="at0024">
          <items id="description">Moderat</items>
          <items id="text">Score 2 - En pasient med tydelig redusert allmenntilstand pga sin sykdom.</items>
        </term_definitions>
        <term_definitions code="at0025">
          <items id="description">Alvorlig</items>
          <items id="text">Score 3 - En pasient som er alvorlig syk.</items>
        </term_definitions>
        <term_definitions code="at0027">
          <items id="description">Er pasienten 70 år eller eldre?</items>
          <items id="text">Er pasienten 70 år eller eldre?</items>
        </term_definitions>
        <term_definitions code="at0028">
          <items id="description">Pasienten er yngre enn 70 år.</items>
          <items id="text">Score 0 - Nei</items>
        </term_definitions>
        <term_definitions code="at0029">
          <items id="description">Pasienten er 70 år eller eldre.</items>
          <items id="text">Score 1 - Ja</items>
        </term_definitions>
        <term_definitions code="at0030">
