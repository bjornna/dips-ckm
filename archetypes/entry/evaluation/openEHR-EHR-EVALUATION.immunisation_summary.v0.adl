definition
	EVALUATION[at0000] matches {	-- Vaksinasjonssammendrag
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0019] occurrences matches {0..1} matches {	-- Samlet vaksinasjonsstatus
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0021, 	-- Ingen vaksinasjoner anbefalt
									at0022, 	-- Vaksinasjoner nødvendig
									at0023]	-- Alle vaksinasjoner avslått
								}
							}
						}
					}
					ELEMENT[at0025] occurrences matches {0..1} matches {	-- Beskrivelse
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0016] occurrences matches {0..*} matches {	-- Spesifikk vaksine
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0002] occurrences matches {1..*} matches {	-- Infeksjonssykdom eller agens
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0010] occurrences matches {0..1} matches {	-- Vaksinasjonsstatus
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0011, 	-- Vaksine ikke nødvendig
											at0012, 	-- Vaksinasjon nødvendig
											at0017, 	-- Vaksinasjon ikke indisert
											at0018]	-- Vaksinasjon avslått
										}
									}
								}
							}
							ELEMENT[at0003] occurrences matches {0..1} matches {	-- Status på primærvaksinasjon
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0004, 	-- Ikke påbegynt
											at0005, 	-- Ufullstendig
											at0006, 	-- Fullført
											at0007]	-- Ukjent
										}
									}
								}
							}
							ELEMENT[at0008] occurrences matches {0..1} matches {	-- Dato primærvaksinasjonen ble fullført
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0009] occurrences matches {0..1} matches {	-- Dato for siste påfyllingsvaksine
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0020] occurrences matches {0..1} matches {	-- Neste dose
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0024] occurrences matches {0..1} matches {	-- Kommentar
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0013] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0014] occurrences matches {0..1} matches {	-- Tidspunkt neste gjennomgang
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- Sist oppdatert
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}

