definition
	EVALUATION[at0000] matches {	-- Sammendrag av medisinsk utstyr
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0020] occurrences matches {0..1} matches {	-- Type
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Status
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0004, 	-- Nåværende
									at0005]	-- Tidligere
								}
							}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- Beskrivelse
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0022] occurrences matches {0..*} matches {	-- Detaljer
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0007] occurrences matches {0..1} matches {	-- Navn på medisinsk utstyr
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0008] occurrences matches {0..1} matches {	-- Start dato
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0012] occurrences matches {0..1} matches {	-- Anatomisk lokalisering
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0013] occurrences matches {0..*} matches {	-- Strukturert anatomisk lokalisering
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[at0014] occurrences matches {0..1} matches {	-- Beskrivelse
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0010] occurrences matches {0..*} matches {	-- Strukturerte detaljer
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
							}
							allow_archetype CLUSTER[at0021] occurrences matches {0..*} matches {	-- Multimedia
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[at0009] occurrences matches {0..1} matches {	-- Stopp dato
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0023] occurrences matches {0..1} matches {	-- URI til original data
								value matches {
									DV_URI matches {*}
								}
							}
							ELEMENT[at0019] occurrences matches {0..1} matches {	-- Dato for neste gjennomgang
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0016] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0018] occurrences matches {0..*} matches {	-- Utvidelse
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {	-- Sist oppdatert
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}


