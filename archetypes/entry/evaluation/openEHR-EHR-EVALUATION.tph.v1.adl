					}
					ELEMENT[at0005] occurrences matches {0..*} matches {	-- Det er framsatt begjæring (regelsett; info klageadgang)
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0007] occurrences matches {0..*} matches {	-- Dato begjæringen ble fremmet
						value matches {
							DV_DATE matches {
								value matches {yyyy-??-XX}
							}
						}
					}
					allow_archetype CLUSTER[at0072] occurrences matches {0..*} matches {	-- Begjæring framsatt av
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0014] occurrences matches {0..*} matches {	-- Begjæringen er dokumentert i journalnotat datert
						value matches {
