					description = <"*The medication course has been completed as planned.(en)">
				>
				["at0008"] = <
					text = <"*Medication course supply delayed(en)">
					description = <"*The medication course has not been dispensed as supply is not yet available.(en)">
				>
				["at0009"] = <
					text = <"*Medication course suspended(en)">
					description = <"*The administration of the medication has been suspended.(en)">
				>
				["at0010"] = <
					text = <"*Prescription re-issued(en)">
					description = <"*A prescription token has been re-issued for an existing medication order.(en)">
				>
				["at0011"] = <
					text = <"*Prescription re-issue suspended(en)">
					description = <"*Reissuing of this prescription is not available without some intervention.(en)">
				>
				["at0012"] = <
					text = <"*Medication course cancelled(en)">
					description = <"*The planned administration has been cancelled prior to any administration.(en)">
				>
				["at0013"] = <
					text = <"*Scheduled medication postponed(en)">
					description = <"*A scheduled medication has been postponed prior to any administration.(en)">
				>
