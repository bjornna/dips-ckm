definition
	INSTRUCTION[at0000.1] matches {	-- Prosedyrerekvirering
		activities cardinality matches {0..*; unordered} matches {
			ACTIVITY[at0001] occurrences matches {1..*} matches {	-- Rekvirering
				description matches {
					ITEM_TREE[at0009] matches {	-- Tree
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0121.1] matches {	-- Prosedyrenavn
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0.2] occurrences matches {0..1} matches {	-- Prosedyretype
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0135.1] occurrences matches {0..1} matches {	-- Prosedyrebeskrivelse
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0132] occurrences matches {0..*} matches {	-- Spesifikke detaljer
								include
									archetype_id/value matches {/.*/}
							}
							ELEMENT[at0062] occurrences matches {0..1} matches {	-- Rekvireringsårsak
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0064] occurrences matches {0..1} matches {	-- Årsaksbeskrivelse
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0065.1] occurrences matches {0..1} matches {	-- Hensikt
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0068] occurrences matches {0..1} matches {	-- Hastegrad
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0136, 	-- Akutt
											at0137, 	-- Haster
											at0138]	-- Rutine
										}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0146.1] occurrences matches {0..1} matches {	-- Ønsket dato/tid for start av prosedyre
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0145.1] occurrences matches {0..1} matches {	-- Tidligste dato/tid for start av prosedyre
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0040.1] occurrences matches {0..1} matches {	-- Ønsket dato/tid for fullført prosedyre
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0144.1] occurrences matches {0..1} matches {	-- Seneste dato/tid for fullført prosedyre
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0076] occurrences matches {0..1} matches {	-- Supplerende informasjon følger
								value matches {
									DV_BOOLEAN matches {
										value matches {True}
									}
								}
							}
							ELEMENT[at0078] occurrences matches {0..1} matches {	-- Detaljer om følgende supplerende informasjon
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0116] occurrences matches {0..*} matches {	-- Pasientens behov
								include
									archetype_id/value matches {/.*/}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0008] matches {	-- Tree
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0010.1] occurrences matches {0..1} matches {	-- Rekvisisjonsidentifikator
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0141] occurrences matches {0..*} matches {	-- Rekvirent
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {	-- Mottakers rekvisisjonsidentifikator
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0142] occurrences matches {0..*} matches {	-- Mottaker
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0127.1] occurrences matches {0..1} matches {	-- Rekvisisjonsstatus
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0128] occurrences matches {0..*} matches {	-- Svarmottakere
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.distribution\.v1/}
					}
					allow_archetype CLUSTER[at0112] occurrences matches {0..*} matches {	-- Tilleggsinformasjon
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

