									allow_archetype CLUSTER[at0170] occurrences matches {0..*} matches {	-- Utdelingsdetaljer
										include
											archetype_id/value matches {/.*/}
									}
								}
							}
							CLUSTER[at0133] occurrences matches {0..1} matches {	-- Administrasjonsveiledning
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0108] occurrences matches {0..*} matches {	-- Administrasjonsinstruksjon
										value matches {
