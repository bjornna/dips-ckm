										}
									}
									ELEMENT[at0136] occurrences matches {0..1} matches {	-- Formål med infusjon
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0137, 	-- Væske/elektrolyttilførsel
													at0138, 	-- Legemiddelinfusjon
													at0163]	-- Ernæringsinfusjon
												}
											}
										}
									}
									ELEMENT[at0092] occurrences matches {0..1} matches {	-- Kroppssted
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0093] occurrences matches {0..1} matches {	-- Strukturert anatomisk lokalisering
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_clock(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.anatomical_location_relative(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0094] occurrences matches {0..*} matches {	-- Administrasjonsmåte
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0095] occurrences matches {0..*} matches {	-- Detaljer om administrasjonsutstyr
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
									}
								}
							}
							allow_archetype CLUSTER[at0166] occurrences matches {0..*} matches {	-- Tilleggsinformasjon
