			purpose = <"To register a child's development during puberty.">
			use = <"Registration of genial, breast and pubic hair development.">
			misuse = <"Use of breast registration on males and genital registration on females.">
			copyright = <"">
		>
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"Å registrere et barns pubertetsutvikling.">
			use = <"Registrere genitalia, pubes og brystutvikling på barn og ungdom.">
			misuse = <"Det skal ikke registreres brystutvikling på gutter/menn.
Det skal ikke registreres genitaliautvikling på jenter/kvinner.">
