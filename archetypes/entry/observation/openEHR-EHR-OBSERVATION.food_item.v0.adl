definition
	OBSERVATION[at0000] matches {	-- Food item
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Food item name
										value matches {
											DV_TEXT matches {*}
										}
									}
									CLUSTER[at0005] occurrences matches {0..1} matches {	-- Amount
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0006] occurrences matches {0..1} matches {	-- Mass
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::124]>
														list = <
															["1"] = <
																units = <"g">
																magnitude = <|>=0.0|>
															>
															["2"] = <
																units = <"kg">
																magnitude = <|>=0.0|>
															>
															["3"] = <
																units = <"[oz_av]">
																magnitude = <|>=0.0|>
															>
															["4"] = <
																units = <"[lb_av]">
																magnitude = <|>=0.0|>
															>
														>
													>
												}
											}
											ELEMENT[at0007] occurrences matches {0..1} matches {	-- Volume
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::129]>
														list = <
															["1"] = <
																units = <"ml">
																magnitude = <|>=0.0|>
															>
															["2"] = <
																units = <"cl">
																magnitude = <|>=0.0|>
															>
															["3"] = <
																units = <"dl">
																magnitude = <|>=0.0|>
															>
															["4"] = <
																units = <"l">
																magnitude = <|>=0.0|>
															>
															["5"] = <
																units = <"[foz_us]">
																magnitude = <|>=0.0|>
															>
															["6"] = <
																units = <"[pt_us]">
																magnitude = <|>=0.0|>
															>
															["7"] = <
																units = <"[tsp_m]">
																magnitude = <|>=0.0|>
															>
															["8"] = <
																units = <"[tsp_us]">
																magnitude = <|>=0.0|>
															>
															["9"] = <
																units = <"[tbs_m]">
																magnitude = <|>=0.0|>
															>
															["10"] = <
																units = <"[tbs_us]">
																magnitude = <|>=0.0|>
															>
															["11"] = <
																units = <"[cup_m]">
																magnitude = <|>=0.0|>
															>
															["12"] = <
																units = <"[cup_us]">
																magnitude = <|>=0.0|>
															>
														>
													>
												}
											}
											ELEMENT[at0008] occurrences matches {0..1} matches {	-- Serving
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::380]>
														list = <
															["1"] = <
																units = <"1">
																magnitude = <|>=0.0|>
																precision = <|1|>
															>
														>
													>
												}
											}
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- Glycaemic load
										value matches {
											DV_COUNT matches {
												magnitude matches {|>=0|}
											}
										}
									}
									allow_archetype CLUSTER[at0010] occurrences matches {0..*} matches {	-- Nutrients
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.macronutrients(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.micronutrients(-[a-zA-Z0-9_]+)*\.v0/}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {	-- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
					INTERVAL_EVENT[at0023] occurrences matches {0..*} matches {	-- One day
						width matches {
							DV_DURATION matches {
								value matches {|PT24H|}
							}
						}
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]	-- /data[Event Series]/events[Any event]/data[Tree]
						}
					}
					INTERVAL_EVENT[at0024] occurrences matches {0..*} matches {	-- One week
						width matches {
							DV_DURATION matches {
								value matches {|P1W|}
							}
						}
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]	-- /data[Event Series]/events[Any event]/data[Tree]
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0012] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0013] occurrences matches {0..*} matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {	-- Serving size (mass)
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::124]>
								list = <
									["1"] = <
										units = <"g">
										magnitude = <|>=0.0|>
									>
									["2"] = <
										units = <"kg">
										magnitude = <|>=0.0|>
									>
									["3"] = <
										units = <"[oz_av]">
										magnitude = <|>=0.0|>
									>
									["4"] = <
										units = <"[lb_av]">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- Serving size (volume)
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::129]>
								list = <
									["1"] = <
										units = <"ml">
										magnitude = <|>=0.0|>
									>
									["2"] = <
										units = <"cl">
										magnitude = <|>=0.0|>
									>
									["3"] = <
										units = <"dl">
										magnitude = <|>=0.0|>
									>
									["4"] = <
										units = <"l">
										magnitude = <|>=0.0|>
									>
									["5"] = <
										units = <"[foz_us]">
										magnitude = <|>=0.0|>
									>
									["6"] = <
										units = <"[pt_us]">
										magnitude = <|>=0.0|>
									>
									["7"] = <
										units = <"[tsp_m]">
										magnitude = <|>=0.0|>
									>
									["8"] = <
										units = <"[tsp_us]">
										magnitude = <|>=0.0|>
									>
									["9"] = <
										units = <"[tbs_m]">
										magnitude = <|>=0.0|>
									>
									["10"] = <
										units = <"[tbs_us]">
										magnitude = <|>=0.0|>
									>
									["11"] = <
										units = <"[cup_m]">
										magnitude = <|>=0.0|>
									>
									["12"] = <
										units = <"[cup_us]">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0016] occurrences matches {0..1} matches {	-- Method
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0020, 	-- Memory
									at0021]	-- Observation
								}
							}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {	-- Accuracy
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0018, 	-- Estimated
									at0019]	-- Measured
								}
							}
						}
					}
					allow_archetype CLUSTER[at0017] occurrences matches {0..*} matches {	-- Device
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}



ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
