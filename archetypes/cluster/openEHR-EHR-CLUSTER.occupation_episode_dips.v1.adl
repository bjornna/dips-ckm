definition
	CLUSTER[at0000] matches {	-- Sysselsettingsepisode
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {	-- Kategori
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0016] occurrences matches {0..1} matches {	-- Beskrivelse
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {	-- Dato for oppstart
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			allow_archetype CLUSTER[at0004] occurrences matches {0..*} matches {	-- Organisasjonsdetaljer
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0006] occurrences matches {0..1} matches {	-- Profesjon
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0005] occurrences matches {0..*} matches {	-- Tittel/rolle
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0013] occurrences matches {0..1} matches {	-- Stillingsandel
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::380]>
						list = <
							["1"] = <
								units = <"%">
							>
						>
					>
				}
			}
			ELEMENT[at0019] occurrences matches {0..1} matches {	-- Arbeidstimer
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::709]>
						list = <
							["1"] = <
								units = <"h/d">
								magnitude = <|>=0.0|>
								precision = <|1|>
							>
							["2"] = <
								units = <"h/wk">
								magnitude = <|>=0.0|>
								precision = <|1|>
							>
							["3"] = <
								units = <"h/mo">
								magnitude = <|>=0.0|>
								precision = <|1|>
							>
							["4"] = <
								units = <"h/a">
								magnitude = <|>=0.0|>
								precision = <|1|>
							>
						>
					>
				}
			}
			ELEMENT[at0015] occurrences matches {0..1} matches {	-- Status
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0018] occurrences matches {0..*} matches {	-- Ytterligere detaljer
				include
					archetype_id/value matches {/.*/}
			}
			ELEMENT[at0008] occurrences matches {0..1} matches {	-- Dato opphør
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0014] occurrences matches {0..1} matches {	-- Kommentar
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Occupation episode">
					description = <"Detailed information about the daily activity of an individual during a specified period of time.">
					comment = <"For example: employed, unemployed, carer, student and retired.">
				>
				["at0001"] = <
					text = <"Category">
					description = <"The category of the occupation episode.">
					comment = <"For example: employed, unemployed, student, retired, etc. Coding with a terminology is desirable, where possible. 'Unemployed' is strictly speaking not an occupation, but it's still relevant as an example for this element, as in some situations it may be necessary to document episodes of unemployment.">
				>
				["at0004"] = <
					text = <"Organisation details">
					description = <"Further details about the employer or educational institution.">
				>
				["at0005"] = <
					text = <"Title/role">
					description = <"Job title or role of the occupation episode.">
				>
				["at0006"] = <
					text = <"Profession">
					description = <"Classification of the type of profession undertaken by the individual.">
					comment = <"For example: categorisation according to the ISCO-08(http://www.ilo.org/global/publications/ilo-bookstore/order-online/books/WCMS_172572/lang--en/index.htm) major, sub-major, minor and unit groups..">
				>
				["at0007"] = <
					text = <"Date commenced">
					description = <"Date occupation episode commenced.">
				>
				["at0008"] = <
					text = <"Date ceased">
					description = <"Date occupation episode ceased.">
				>
				["at0013"] = <
					text = <"Full time equivalent">
					description = <"The position's fraction or percentage of a full time equivalent.">
				>
				["at0014"] = <
					text = <"Comment">
					description = <"Additional narrative about the occupation episode not captured in other fields.">
				>
				["at0015"] = <
					text = <"Status">
					description = <"The current status for the occupation episode.">
					comment = <"For example: on leave or sick leave, seconded, temporarily laid off. Coding with a terminology is desirable, where possible.">
				>
				["at0016"] = <
					text = <"Description">
					description = <"Narrative description of the occupation episode.">
				>
				["at0018"] = <
					text = <"Additional details">
					description = <"Further detail about the occupation episode.">
					comment = <"For example information about income or workplace conditions.">
				>
				["at0019"] = <
					text = <"Hours">
					description = <"The number of hours spent per time period at the specified occupation.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Sysselsettingsepisode">
					description = <"Detaljert informasjon om et individs daglige aktivitet i et spesifikt tidsintervall.">
					comment = <"For eksempel: i arbeid, arbeidsløs, hjemmeværende, student, eller pensjonist.">
				>
				["at0001"] = <
					text = <"Kategori">
					description = <"Kategorien for sysselsettingsepisoden.">
					comment = <"For eksempel i arbeid, arbeidsløs, student, pensjonert, etc. Koding med en terminologi er ønskelig, der det er mulig. \"Arbeidsløs\" er strengt tatt ikke en kategori av sysselsetting, men er fremdeles relevant for dette elementet, da det i noen situasjoner er nødvendig å dokumentere tidsrom da individet har vært arbeidsløs.">
				>
				["at0004"] = <
					text = <"Organisasjonsdetaljer">
					description = <"Ytterligere detaljer om arbeidsgiver eller utdanningsinstitusjon.">
				>
				["at0005"] = <
					text = <"Tittel/rolle">
					description = <"Stillingstittel eller rolle for sysselsettingsepisoden.">
				>
				["at0006"] = <
					text = <"Profesjon">
					description = <"Klassifisering av typen profesjon individet har.">
					comment = <"For eksempel kategorisering i henhold til STYRK-08 (https://www.ssb.no/arbeid-og-lonn/artikler-og-publikasjoner/standard-for-yrkesklassifisering-styrk-08) \"yrkesfelt\", \"yrkesområder\", \"yrkesgrupper\" og \"yrker\".">
				>
				["at0007"] = <
					text = <"Dato for oppstart">
					description = <"Datoen sysselsettingsepisoden startet.">
				>
				["at0008"] = <
					text = <"Dato opphør">
					description = <"Datoen sysselsettingsepisoden opphørte.">
				>
				["at0013"] = <
					text = <"Stillingsandel">
					description = <"Stillingens andel av en fulltidsstilling.">
				>
				["at0014"] = <
					text = <"Kommentar">
					description = <"Ytterligere kommentar til sysselsettingsepisoden som ikke er fanget i andre felt.">
				>
				["at0015"] = <
					text = <"Status">
					description = <"Individets status for den aktuelle sysselsettingsepisoden.">
					comment = <"For eksempel sykemeldt, i aktiv sykemelding, i permisjon, utlånt eller permittert. Koding med en terminologi er ønskelig der det er mulig.">
				>
				["at0016"] = <
					text = <"Beskrivelse">
					description = <"Fritekstbeskrivelse av sysselsettingsepisoden.">
				>
				["at0018"] = <
					text = <"Ytterligere detaljer">
					description = <"Ytterligere detaljer om sysselsettingsepisoden.">
					comment = <"For eksempel informasjon om inntekt eller arbeidsforhold.">
				>
				["at0019"] = <
					text = <"Arbeidstimer">
					description = <"Antall timer per tidsenhet individet bruker på den aktuelle sysselsettingsepisoden.">
				>
			>
		>
	>
