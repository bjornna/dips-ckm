			purpose = <"Used to describe the resource needed to peform a specific type of healthcare service ">
			use = <"Used primarily in connection with planning activities in order to be able to specify the type of resources needed. Eg. when performing an operation.">
			keywords = <"*ressurs(nb)", "*helsetjeneste(nb)">
