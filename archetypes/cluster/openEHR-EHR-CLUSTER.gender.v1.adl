			ELEMENT[at0020] occurrences matches {0..1} matches {	-- Foretrukket pronomen
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0023] occurrences matches {0..*} matches {	-- Detaljer
