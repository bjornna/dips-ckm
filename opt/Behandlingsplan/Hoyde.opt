        <term_definitions code="at0000">
          <items id="description">Individets høyde eller lengde målt fra isse til fotsåle. Høyde måles stående og lengde liggende.</items>
          <items id="text">Høyde/Lengde</items>
        </term_definitions>
        <term_definitions code="at0001">
          <items id="description">@ internal @</items>
          <items id="text">history</items>
        </term_definitions>
        <term_definitions code="at0002">
          <items id="description">Standard, uspesifisert tidspunkt eller tidsintervall som kan defineres mer eksplisitt i en template eller i en applikasjon.</items>
          <items id="text">Uspesifisert hendelse</items>
        </term_definitions>
        <term_definitions code="at0003">
          <items id="description">@ internal @</items>
          <items id="text">Simple</items>
        </term_definitions>
        <term_definitions code="at0004">
          <items id="description">Høyde/lengde fra isse til fotsåle.</items>
          <items id="text">Høyde/lengde</items>
        </term_definitions>
        <term_definitions code="at0007">
          <items id="description">@ internal @</items>
          <items id="text">List</items>
        </term_definitions>
        <term_definitions code="at0011">
          <items id="description">Beskrivelse av måleutstyret brukt til måling av høyde eller lengde.</items>
          <items id="text">Måleutstyr</items>
        </term_definitions>
        <term_definitions code="at0013">
          <items id="description">@ internal @</items>
          <items id="text">Tree</items>
        </term_definitions>
        <term_definitions code="at0014">
          <items id="description">Individets kroppsstilling på måletidspunktet.</items>
          <items id="text">Kroppsstilling</items>
        </term_definitions>
        <term_definitions code="at0016">
          <items id="description">Høyde måles optimalt uten sko, uten sokker (for å se fotstillingen skikkelig), rumpen inntil veggen, skuldrene inntil veggen, hodet i Frankfurt stilling (nedre orbitalkant horisontalt med ytre øregang). En skal si til vedkommende "stå rett", men ikke skyve opp hodet.</items>
          <items id="text">Stående</items>
        </term_definitions>
        <term_definitions code="at0018">
          <items id="description">Ytterligere beskrivelse av målingen av høyde/lengde som ikke dekkes i andre felt.</items>
          <items id="text">Kommentar</items>
        </term_definitions>
        <term_definitions code="at0019">
          <items id="description">Registrering av faktorer som kan ha innflytelse på måling av kroppslengden f.eks. notat om amputasjon, lue, håroppsett, klemmer eller lignende.</items>
          <items id="text">Konfunderende faktorer</items>
        </term_definitions>
        <term_definitions code="at0020">
          <items id="description">Lengde måles i en helt utstrakt, liggende posisjon med bekkenet flatt, beina strekte og føtter verken flektert eller ekstendert.</items>
          <items id="text">Liggende</items>
        </term_definitions>
        <term_definitions code="at0021">
          <items id="description">Den første lengden målt etter fødselen. Denne hendelsen skal kun benyttes én gang per journal.</items>
          <items id="text">Fødsel</items>
        </term_definitions>
        <term_definitions code="at0025">
          <items id="description">Matematisk formel for hvordan høyde/lengde registrert i feltet "Justert høyde/lengde" er beregnet.</items>
          <items id="text">Justeringsmetode</items>
        </term_definitions>
        <term_definitions code="at0026">
          <items id="description">Justert høyde/lengde er beregnet basert på en målt høyde/lengde, armspenn, underarmslengde eller lignende. Justert høyde/lengde brukes til bl.a. beregning av BMI hvis pasienten mangler kroppsdeler som signifikant påvirker målt høyde/lengde.</items>
          <items id="text">Justert høyde/lengde</items>
        </term_definitions>
        <term_definitions code="at0028">
          <items id="description">Begrunnelse for hvorfor feltet "Justert høyde/lengde" er benyttet.</items>
          <items id="text">Begrunnelse for justering</items>
        </term_definitions>
