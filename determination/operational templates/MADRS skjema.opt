        <items id="text">3 - mellom 2 og 4</items>
      </items>
      <items code="at0067">
        <items id="description">*</items>
        <items id="text">4 - Tap av interesse for omverdenen. Tap av følelser for venner og kjente</items>
      </items>
      <items code="at0068">
