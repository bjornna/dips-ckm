      <items code="at0071">
        <items id="description">*</items>
        <items id="text">1 - mellom 0 og 2</items>
      </items>
      <items code="at0072">
        <items id="description">*</items>
        <items id="text">2 - Fluktuerende tanker om ikke å strekke til, selvbebreidelser og mindreverdsfølelse</items>
