          <items id="comment">Perloff D, Grim C, Flack J, Frohlich ED, Hill M, McDonald M, Morgenstern BZ. Human blood pressure determination by sphygmomanometry. Circulation 1993;88;2460-2470.</items>
          <items id="description">Størrelse på blodtrykkmansjetten som anvendes til måling av blodtrykket. (Cuffstørrelse)</items>
          <items id="text">Mansjettstørrelse</items>
        </term_definitions>
        <term_definitions code="at0014">
          <items id="description">Anatomisk sted hvor blodtrykket måles. Bruk listen over interne koder for valg av vanlige målesteder. Bruk fri eller kodet tekst for registrering av mer spesifikke detaljer eller et målested som ikke inngår i listen over vanlige steder. Henvis eventuelt til ekstern terminologi.</items>
          <items id="text">Målested</items>
        </term_definitions>
        <term_definitions code="at0015">
          <items id="description">Mansjett for bruk rundt låret til voksne individer.</items>
          <items id="text">Lår voksne</items>
        </term_definitions>
        <term_definitions code="at0016">
          <items id="description">En blodtrykksmansjett for voksne med store armer.</items>
          <items id="text">Store voksne</items>
        </term_definitions>
        <term_definitions code="at0017">
          <items id="description">En standard blodtrykksmansjett til voksne.</items>
          <items id="text">Voksne</items>
        </term_definitions>
        <term_definitions code="at0025">
          <items id="description">Individets høyre overarm.</items>
          <items id="text">Overarm, høyre</items>
        </term_definitions>
        <term_definitions code="at0026">
          <items id="description">Individets venstre overarm.</items>
          <items id="text">Overarm, venstre</items>
        </term_definitions>
        <term_definitions code="at0027">
          <items id="description">Individets høyre lår.</items>
          <items id="text">Lår, høyre</items>
        </term_definitions>
        <term_definitions code="at0028">
          <items id="description">Individets venstre lår.</items>
          <items id="text">Lår, venstre</items>
        </term_definitions>
        <term_definitions code="at0033">
          <items id="description">Kommentar til blodtrykksmålingen.</items>
          <items id="text">Kommentar</items>
        </term_definitions>
        <term_definitions code="at1000">
          <items id="description">Stående ved tidspunktet for målingen.</items>
          <items id="text">Stående</items>
        </term_definitions>
        <term_definitions code="at1001">
          <items id="description">Sittende (for eksempel på en stol eller på en seng med føttene på gulvet) på tidspunkt for blodtrykksmålingen.</items>
          <items id="text">Sittende</items>
        </term_definitions>
        <term_definitions code="at1002">
          <items id="description">Sittende tilbakelent ca 45º og med beina hevet til samme høyde som hoften på tidspunkt for blodtrykksmålingen.</items>
          <items id="text">Tilbakelent</items>
        </term_definitions>
        <term_definitions code="at1003">
          <items id="description">Liggende flatt på tidspunkt for blodtrykksmålingen.</items>
          <items id="text">Liggende</items>
        </term_definitions>
        <term_definitions code="at1005">
          <items id="comment">Negative verdier betegner posisjoner med hodet ned.</items>
          <items id="description">Kranio-caudal tilt på tidspunkt for blodtrykksmåling.</items>
          <items id="text">Tilt</items>
        </term_definitions>
        <term_definitions code="at1006">
          <items id="description">Det gjennomsnittlige trykket mot arterieveggen gjennom en enkelt hjertesyklus. Middeltrykk/MAP måles direkte eller beregnes ut fra systolisk og diastolisk trykk ved hjelp av en matematisk formel.</items>
          <items id="text">Middelarterietrykk</items>
        </term_definitions>
        <term_definitions code="at1007">
          <items id="description">Differansen mellom det systoliske og diastoliske arterielle blodtrykket.</items>
          <items id="text">Pulstrykk</items>
        </term_definitions>
        <term_definitions code="at1008">
          <items id="description">En blodtrykksmansjett for små voksne.</items>
          <items id="text">Små voksne</items>
        </term_definitions>
        <term_definitions code="at1009">
          <items id="description">En blodtrykksmansjett til barn eller små voksne med tynne armer.</items>
          <items id="text">Barn</items>
        </term_definitions>
        <term_definitions code="at1010">
          <items id="description">Registrering av hvilken av Korotkofflydene som brukes for å bestemme det diastoliske blodtrykket ved hjelp av auskultasjon.</items>
          <items id="text">Diastolisk endepunkt</items>
        </term_definitions>
        <term_definitions code="at1011">
          <items id="description">Den fjerde Korotkoff lyd som identifieres som en brå demping av lydene.</items>
          <items id="text">Fase IV</items>
        </term_definitions>
        <term_definitions code="at1012">
          <items id="description">Den femte Korotkoff lyd som identifiseres som fraværet av lyder idet mansjettrykket faller under det diastoliske blodtrykket.</items>
          <items id="text">Fase V</items>
        </term_definitions>
        <term_definitions code="at1014">
          <items id="description">Liggende flatt med noe lateral tilt, vanligvis vinklet mot venstre side. Vanligvis nødvendig i siste trimester av svangerskapet for å avlaste aortocaval komprimering.</items>
          <items id="text">Liggende lent mot venstre</items>
        </term_definitions>
        <term_definitions code="at1018">
          <items id="description">En blodtrykksmansjett til bruk for spedbarn.</items>
          <items id="text">Spedbarn</items>
        </term_definitions>
        <term_definitions code="at1019">
          <items id="description">En blodtrykksmansjett til bruk for neonatale, forutsatt at blodtrykksmansjetten er tilpasset i størrelse for modenhet og fødselsvekt.</items>
          <items id="text">Neonatale</items>
        </term_definitions>
        <term_definitions code="at1020">
          <items id="description">Individets høyre håndledd.</items>
          <items id="text">Håndledd, høyre</items>
        </term_definitions>
        <term_definitions code="at1021">
          <items id="description">Individets venstre håndledd.</items>
          <items id="text">Håndledd, venstre</items>
        </term_definitions>
        <term_definitions code="at1025">
          <items id="description">Detaljer om sfygmomanometeret eller annet måleapparat brukt til blodtrykksmåling.</items>
          <items id="text">Måleapparat</items>
        </term_definitions>
        <term_definitions code="at1026">
          <items id="description">Individets høyre ankel.</items>
          <items id="text">Ankel, høyre</items>
        </term_definitions>
        <term_definitions code="at1030">
          <items id="description">Detaljer om fysisk aktivitet på tidspunkt for blodtrykksmåling.</items>
          <items id="text">Fysisk anstrengelse</items>
        </term_definitions>
        <term_definitions code="at1031">
          <items id="description">Individets venstre ankel.</items>
          <items id="text">Ankel, venstre</items>
        </term_definitions>
        <term_definitions code="at1032">
          <items id="description">Individets finger. Dersom identifikasjon av den spesifikke fingeren er nødvendig, kan dette legges inn ved hjelp av feltet med fri eller kodet tekst.</items>
          <items id="text">Finger</items>
        </term_definitions>
        <term_definitions code="at1035">
          <items id="description">Metode for måling av blodtrykket.</items>
          <items id="text">Målemetode</items>
        </term_definitions>
        <term_definitions code="at1036">
          <items id="description">Metode for måling av blodtrykk eksternt, ved hjelp av stetoskop og Korotkofflyder.</items>
          <items id="text">Auskultasjon</items>
        </term_definitions>
        <term_definitions code="at1037">
          <items id="description">Metode for måling av blodtrykket eksternt, ved hjelp av palpasjon (vanligvis av arteriene brachialis eller radialis).</items>
          <items id="text">Palpasjon</items>
        </term_definitions>
        <term_definitions code="at1038">
          <items id="description">Eventuell formel som er anvendt til beregning av middeltrykket (MAP).</items>
          <items id="text">Formel for beregning av MAP</items>
        </term_definitions>
        <term_definitions code="at1039">
          <items id="description">Metode for non-invasiv måling av blodtrykket ved hjelp av en maskin som automatisk måler blodtrykket, for eksempel et måleapparat eller en blodtrykksmåler for hjemmebruk.</items>
          <items id="text">Automatisk, non-invasivt</items>
        </term_definitions>
        <term_definitions code="at1040">
          <items id="description">Metode for måling av blodtrykket invasivt med innleggelse av intravasalkateter i en blodåre.</items>
          <items id="text">Automatisk, invasivt</items>
        </term_definitions>
        <term_definitions code="at1042">
          <items id="description">Estimert gjennomsnittsblodtrykk over en 24-timers periode.</items>
          <items id="text">24-timers gjennomsnitt</items>
        </term_definitions>
        <term_definitions code="at1043">
          <items id="description">Søvnstatus - understøtter tolkning av 24 timers ambulant blodtrykksmåling.</items>
          <items id="text">Søvnstatus</items>
        </term_definitions>
        <term_definitions code="at1044">
          <items id="description">Individet er ved full bevissthet.</items>
          <items id="text">Våken</items>
        </term_definitions>
        <term_definitions code="at1045">
          <items id="description">Individet er i den naturlige tilstand av kroppslig hvile.</items>
          <items id="text">Sovende</items>
        </term_definitions>
        <term_definitions code="at1051">
          <items id="description">Individets tå. Dersom identifikasjon av den spesifikke tåen er nødvendig, kan dette legges inn ved hjelp av feltet med fri eller kodet tekst.</items>
          <items id="text">Tå</items>
        </term_definitions>
        <term_definitions code="at1052">
          <items id="description">Kommentar til og registrering av andre faktorer som kan påvirke blodtrykksmålingen. For eksempel angst eller "hvit frakk syndrom", smerter eller feber, endringer i atmosfærisk trykk osv.</items>
          <items id="text">Konfunderende faktorer</items>
        </term_definitions>
        <term_definitions code="at1054">
          <items id="description">Formelen som anvendes til beregning av systolisk blodtrykk dersom det benyttes en maskin som måler middeltrykket og beregner systolisk og diastolisk trykk.</items>
          <items id="text">Formel for beregning av systolisk trykk</items>
        </term_definitions>
        <term_definitions code="at1055">
          <items id="description">Formelen som anvendes til beregning av diastolisk blodtrykk dersom det benyttes en maskin som måler middeltrykket og beregner systolisk og diastolisk trykk.</items>
          <items id="text">Formel for beregning av diastolisk trykk</items>
        </term_definitions>
        <term_definitions code="at1056">
          <items id="description">Individets fotrygg.</items>
          <items id="text">Fotrygg</items>
        </term_definitions>
