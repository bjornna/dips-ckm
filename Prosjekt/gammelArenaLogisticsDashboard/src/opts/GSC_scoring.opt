        <archetype_id>
          <value>openEHR-EHR-OBSERVATION.glasgow_coma_scale.v1</value>
        </archetype_id>
        <term_definitions code="at0000">
          <items id="description">En femtenpoengs skala som brukes for å anslå bevissthetsnivå ut fra et definert stimuli.</items>
          <items id="text">Glasgow Coma Scale</items>
        </term_definitions>
        <term_definitions code="at0001">
          <items id="description">@ internal @</items>
          <items id="text">Event Series</items>
        </term_definitions>
        <term_definitions code="at0002">
          <items id="description">Spesifisert tidspunkt eller tidsintervall som kan defineres mer eksplisitt i en templat eller i en applikasjon.</items>
          <items id="text">Tidspunkt</items>
        </term_definitions>
        <term_definitions code="at0003">
          <items id="description">@ internal @</items>
          <items id="text">Tree</items>
        </term_definitions>
        <term_definitions code="at0007">
          <items id="comment">Vanligvis vil skår for verbalrespons velges fra en av ordinalverdiene, men i tilfeller hvor verbalrespons ikke kan testes, for eksempel om pasienten er intubert, skal null-verdien &quot;Not Applicable&quot; registreres.</items>
          <items id="description">Beste verbal respons på stimuli.</items>
          <items id="text">Verbal respons (V)</items>
        </term_definitions>
        <term_definitions code="at0008">
          <items id="comment">Vanligvis vil skår for motorisk respons velges fra en av ordinalverdiene, men i tilfeller hvor en respons ikke kan testes, for eksempel om pasienten ikke kan bevege lemmene som følge av skade eller lammelse, skal null-verdien &quot;Not Applicable&quot; registreres.</items>
          <items id="description">Beste motoriske respons på stimuli.</items>
          <items id="text">Motorisk respons (M)</items>
        </term_definitions>
        <term_definitions code="at0009">
          <items id="comment">Vanligvis vil skår for øyerespons velges fra en av ordinalverdiene, men i tilfeller hvor øyerespons ikke kan testes, for eksempel om pasienten ikke fysisk kan åpne øynene grunnet skade, skal null-verdien &quot;Not Applicable&quot; registreres.</items>
          <items id="description">Beste øyerespons ved stimuli.</items>
          <items id="text">Åpning av øynene (Ø)</items>
        </term_definitions>
        <term_definitions code="at0010">
          <items id="description">Øynene åpnes ikke, ingen forstyrrende faktorer som for eksempel lukkede øyne på grunn av lokal hevelse.</items>
          <items id="text">Ingen reaksjon</items>
        </term_definitions>
        <term_definitions code="at0011">
          <items id="description">Åpner øynene på smerte. For eksempel ved supraorbitalt trykk.</items>
          <items id="text">Ved smertestimulering</items>
        </term_definitions>
        <term_definitions code="at0012">
          <items id="description">Øynene åpnes ved tiltale eller ved roping. Må ikke forveksles med vekking av en sovende person.</items>
          <items id="text">Ved tiltale</items>
        </term_definitions>
        <term_definitions code="at0013">
          <items id="description">Åpner øynene av seg selv uten stimuli.</items>
          <items id="text">Spontant</items>
        </term_definitions>
        <term_definitions code="at0014">
          <items id="description">Ingen hørbar respons og ingen forstyrrende faktorer, som for eksempel intubasjon eller døvhet.</items>
          <items id="text">Ingen reaksjon</items>
        </term_definitions>
        <term_definitions code="at0015">
          <items id="description">Bare stønning/jamring.</items>
          <items id="text">Uforståelige lyder</items>
        </term_definitions>
        <term_definitions code="at0016">
          <items id="description">Forståelige enkeltord.</items>
          <items id="text">Usammenhengende tale</items>
        </term_definitions>
        <term_definitions code="at0017">
          <items id="description">Ikke orientert, men kommuniserer sammenhengende.</items>
          <items id="text">Forvirret, desorientert</items>
        </term_definitions>
        <term_definitions code="at0018">
          <items id="description">Oppgir riktig navn, sted og dato.</items>
          <items id="text">Orientert</items>
        </term_definitions>
        <term_definitions code="at0019">
          <items id="description">Ingen bevegelse i armer/bein, ingen forstyrrende faktorer, som for eksempel lammelser.</items>
          <items id="text">Ingen reaksjon</items>
        </term_definitions>
        <term_definitions code="at0020">
          <items id="description">Decerebrert ekstensjon av armer og/eller bein som respons på stimuli. For eksempel kan armen ekstenderes ved albuen.</items>
          <items id="text">Ekstensjon ved smerte</items>
        </term_definitions>
        <term_definitions code="at0021">
          <items id="description">Abnorm/ unormal fleksjon. Sakte, decortikert fleksjon av armer og/eller bein. For eksempel kan armen bøyes ved albuen, men trekkene er hovedsakelig abnormale.</items>
          <items id="text">Fleksjon ved smerte</items>
        </term_definitions>
        <term_definitions code="at0022">
          <items id="description">Avvergereaksjon. Hurtig fleksjon som respons på stimuli, men trekkene er hovedsakelig normale. For eksempel kan håndleddet bøyes eller trekkes til seg når det utføres supraorbitalt trykk.</items>
          <items id="text">Avverger smerte</items>
        </term_definitions>
        <term_definitions code="at0023">
          <items id="description">Målrettet fleksjon mot smertestimuli. For eksempel kan hånden løftes til over kragebeinet når det utføres supraorbitalt trykk.</items>
          <items id="text">Lokaliserer smerte</items>
        </term_definitions>
        <term_definitions code="at0024">
          <items id="description">Følger oppfordring for bevegelse.</items>
          <items id="text">Følger oppfordringer</items>
        </term_definitions>
        <term_definitions code="at0026">
          <items id="comment">Totalskåren kan regnes ut ved å summere de tre undersøkelsesdelene. Totalskår skal ikke regnes ut når en eller flere undersøkelsesdeler ikke er mulig å gjennomføre, fordi skåren da vil bli kunstig lav. I disse situasjonene skal ØVM-profilen registreres.</items>
          <items id="description">Sum av verdiene som er registrert for hver enkelt av de tre undersøkelsesdelene.</items>
          <items id="text">Totalskår</items>
        </term_definitions>
        <term_definitions code="at0030">
          <items id="comment">ØVM-profilen kan genereres automatisk ved å sette sammen de tre undersøkelsesdelene. Hvis dette gjøres, bør resultatet sammenlignes mellom det som det kliniske informasjonssystemet presenterer og det som klinikere har registrert for å utelukke konflikter eller inkonsistens. Nullverdien &quot;Not applicable&quot; registreres som &quot;IT&quot;, for &quot;ikke testet&quot;, som beskrevet på http://www.glasgowcomascale.org/.</items>
          <items id="description">Alternativ til Totalskår der responsen til Øye, Verbal og Motorisk er uttrykt som tre separate verdier.</items>
          <items id="text">ØVM-profil</items>
        </term_definitions>
        <term_definitions code="at0037">
          <items id="comment">Hvis hele eller deler av testen ikke var gjennomførbar, skal årsak til dette registreres i dataelementet &quot;Konfunderende faktorer&quot;.</items>
          <items id="description">Ytterligere fritekst om målingen som ikke er fanget i andre felt.</items>
          <items id="text">Kommentar</items>
        </term_definitions>
        <term_definitions code="at0038">
          <items id="description">@ internal @</items>
          <items id="text">Tree</items>
        </term_definitions>
        <term_definitions code="at0039">
          <items id="comment">For eksempel: lokale informasjonskrav eller ekstra metadata for å samsvare med FHIR eller CIMI ekvivalenter.</items>
          <items id="description">Ytterligere informasjon som er nødvendig for å sammenstille med andre referansemodeller/formalismer.
</items>
          <items id="text">Utvidelse</items>
        </term_definitions>
        <term_definitions code="at0040">
          <items id="description">@ internal @</items>
          <items id="text">Tree</items>
        </term_definitions>
        <term_definitions code="at0041">
          <items id="comment">For eksempel: Døvhet, afasi, språkproblemer, bruk av beroligende medisin, hypotermi eller paralyser, alvorlige hevelser i ansikt/øyn, alkoholpåvirkning eller trakeal intubasjon som årsaker til at en ikke kunne registrere en eller flere av ordinalene i Ø-, V- eller M-vurderingene.</items>
          <items id="description">Fritekst om faktorer som kan ha bidratt til GCS-skåringene.</items>
          <items id="text">Konfunderende faktorer</items>
        </term_definitions>
        <term_definitions code="at0042">
          <items id="comment">For eksempel: 
&quot;Supraorbitalt trykk&quot; for sentral smertestimulering eller &quot;Trykk på negleroten&quot; for perifer smertestimulering. Trakealsuging og forflytning av pasient er andre eksempler på stimuli.</items>
          <items id="description">Beskrivelse av type smertestimuli.</items>
          <items id="text">Smertestimuli</items>
        </term_definitions>
      </children>
