definition
	EVALUATION[at0000] matches {	-- Problem/diagnose
		data matches {
			ITEM_TREE[at0001] matches {	-- structure
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0002] matches {	-- Problem/diagnosenavn
